{
	"name": "collect-ts",
	"version": "0.4.0",
	"laravelCollectionVersion": "12.43",
	"description": "TypeScript port of Laravel Collection (Laravel 12.x compatible) - fully typed",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"prepare": "lefthook install",
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.test.json",
		"lint": "biome check src tests",
		"format": "biome format --write src tests",
		"coverage": "vitest run --coverage",
		"prepublishOnly": "npm run build",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:check": "tsx scripts/docs/index.ts check",
		"docs:sync": "tsx scripts/docs/index.ts sync"
	},
	"keywords": [
		"laravel",
		"collection",
		"typescript",
		"collect",
		"array",
		"utilities",
		"enumerable"
	],
	"author": "Omid van der Linden",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ovdlinden/collect-ts"
	},
	"homepage": "https://github.com/ovdlinden/collect-ts#readme",
	"bugs": {
		"url": "https://github.com/ovdlinden/collect-ts/issues"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@sebbo2002/semantic-release-jsr": "^3.1.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^7.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"lefthook": "^2.0.13",
		"semantic-release": "^25.0.2",
		"tsup": "^8.5.1",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vitepress": "^1.5.0",
		"vitest": "^4.0.16"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@9.15.1"
}
